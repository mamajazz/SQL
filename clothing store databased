# SQL
SQL Portfolio

-- Create a shop store database

CREATE TABLE store (id INTEGER PRIMARY KEY, sku TEXT, material TEXT, aisle INTEGER, price INTEGER);
INSERT INTO store VALUES (1, "MINKY248", "COTTON", 4, 59);
INSERT INTO store VALUES (2, "cane243", "poly", 5, 69);
INSERT INTO store VALUES (3, "MIiae3", "silk", 6, 59);
INSERT INTO store VALUES (4, "traiw32", "COTTON", 4, 59);

--display the database ordered by price. 
SELECT * FROM store
ORDER BY price desc;

--what is the average price of items in the cotton section? 
SELECT AVG(price) "avg cotton item price"
FROM store
where section='cotton';
